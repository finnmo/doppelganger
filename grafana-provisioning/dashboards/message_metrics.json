          "query": "// Get total messages per sender\ntotals = from(bucket: \"doppelgnger\")\n  |> range(start: v.timeRangeStart)\n  |> filter(fn: (r) => r._measurement == \"emotion_metrics\" and r._field == \"count\")\n  |> group(columns: [\"sender\"])\n  |> sum()\n  |> rename(columns: {_value: \"total\"})\n\n// Get emotion counts and calculate percentages (hide count)\nfrom(bucket: \"doppelgnger\")\n  |> range(start: v.timeRangeStart)\n  |> filter(fn: (r) => r._measurement == \"emotion_metrics\" and r._field == \"count\")\n  |> group(columns: [\"sender\", \"emotion\"])\n  |> sum()\n  |> map(fn: (r) => {\n    total = totals \n      |> filter(fn: (r) => r.sender == r.sender)\n      |> findRecord(fn: (key) => true, idx: 0)\n    \n    return {\n      sender: r.sender,\n      emotion: r.emotion,\n      percentage: (float(v: r._value) / float(v: total.total)) * 100.0\n    }\n  })\n  |> group(columns: [\"sender\"])\n  |> sort(columns: [\"percentage\"], desc: true)\n  |> limit(n: 5)\n  |> group(columns: [\"sender\", \"emotion\"])\n  |> yield(name: \"results\")",
